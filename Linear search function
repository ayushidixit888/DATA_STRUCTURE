#include<stdio.h>
#include<conio.h>
void insertion(int ar[],int n,int item){
    for(int i=0;i<n;i++){
        if(ar[i]==item){
            printf("item found on location %d",i);
        }
        else if(i==n){
            printf("item not found");
        }
    }
}
void main(){
    int ar[100],n,item;
    printf("enter the size");
    scanf("%d",&n);
    printf("enter the item");
    scanf("%d",&item);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&ar[i]);
    }
    linearsearch(ar,n,item);
   
    }


